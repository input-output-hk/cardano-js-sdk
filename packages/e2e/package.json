{
  "name": "@cardano-sdk/e2e",
  "version": "0.3.0",
  "description": "End to end tests for the cardano-js-sdk packages.",
  "main": "index.js",
  "scripts": {
    "test": "shx echo 'test' command not implemented yet",
    "test:blockfrost": "jest -c jest.config.js --selectProjects blockfrost --runInBand --verbose",
    "test:wallet": "jest -c jest.config.js --selectProjects wallet --runInBand --verbose",
    "test:faucet": "jest -c jest.config.js --selectProjects faucet --runInBand --verbose",
    "test:cardano-services": "jest -c jest.config.js --selectProjects cardano-services --runInBand --verbose",
    "test:web-extension:build": "webpack -c test/web-extension/webpack.config.js",
    "test:web-extension:run": "wdio run test/web-extension/wdio.conf.js",
    "test:web-extension": "run-s test:web-extension:build test:web-extension:run",
    "test:web-extension:watch:build": "yarn test:web-extension:build --watch",
    "test:web-extension:watch:run": "yarn test:web-extension:run --watch",
    "test:web-extension:watch": "run-s test:web-extension:build test:web-extension:watch:bg",
    "test:web-extension:watch:bg": "run-p test:web-extension:watch:build test:web-extension:watch:run",
    "local-network:up": "CARDANO_DB_SYNC_VERSION=13.0.0-rc3 DOCKER_BUILDKIT=1 COMPOSE_DOCKER_CLI_BUILD=1 OGMIOS_PORT=1338 NETWORK=testnet docker-compose -p private-testnet-e2e up",
    "local-network:down": "CARDANO_DB_SYNC_VERSION=13.0.0-rc3 DOCKER_BUILDKIT=1 COMPOSE_DOCKER_CLI_BUILD=1 OGMIOS_PORT=1338 NETWORK=testnet docker-compose -p private-testnet-e2e down -v --remove-orphans",
    "build:esm": "tsc -p src/tsconfig.json --outDir ./dist/esm --module es2020",
    "build:cjs": "tsc --build src",
    "build": "run-s build:cjs build:esm module-fixup",
    "module-fixup": "shx cp ../../build/cjs-package.json ./dist/cjs/package.json && cp ../../build/esm-package.json ./dist/esm/package.json",
    "tscNoEmit": "shx echo typescript --noEmit command not implemented yet",
    "cleanup:dist": "shx rm -rf dist",
    "cleanup:nm": "shx rm -rf node_modules",
    "cleanup": "run-s cleanup:dist cleanup:nm",
    "coverage": "yarn test --coverage",
    "lint": "eslint --ignore-path ../../.eslintignore \"**/*.ts\"",
    "lint:fix": "eslint --fix --ignore-path ../../.eslintignore \"**/*.ts\"",
    "prepack": "yarn build",
    "test:build:verify": "tsc --build ./test",
    "test:debug": "DEBUG=true yarn test",
    "generate-mnemonics": "ts-node ./src/Util/mnemonic.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/input-output-hk/cardano-js-sdk/packages/e2e"
  },
  "author": "Angel Castillo",
  "license": "Apache-2.0",
  "dependencies": {
    "@cardano-foundation/ledgerjs-hw-app-cardano": "5.0.0",
    "@cardano-sdk/cardano-services": "0.3.0",
    "@cardano-sdk/core": "0.3.0",
    "@cardano-sdk/util": "0.3.0",
    "@cardano-sdk/util-rxjs": "0.3.0",
    "@cardano-sdk/wallet": "0.3.0",
    "bunyan": "^1.8.15",
    "cardano-wallet-js": "^1.4.0",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "lodash": "^4.17.21",
    "optionator": "^0.9.1",
    "rxjs": "^7.4.0",
    "ts-log": "^2.2.4",
    "ts-node": "^10.8.1",
    "ts-stopwatch": "0.0.4",
    "uuid": "^8.3.2",
    "webextension-polyfill": "^0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@cardano-sdk/util-dev": "0.3.0",
    "@cardano-sdk/wallet": "0.3.0",
    "@emurgo/cardano-message-signing-asmjs": "^1.0.1",
    "@emurgo/cardano-serialization-lib-asmjs": "^9.1.0",
    "@types/bunyan": "^1.8.8",
    "@types/jest": "^28.1.2",
    "@types/lodash": "^4.14.182",
    "@types/uuid": "^8.3.4",
    "@types/webextension-polyfill": "^0.8.0",
    "@vespaiach/axios-fetch-adapter": "^0.3.0",
    "@wdio/cli": "^7.19.5",
    "@wdio/local-runner": "^7.19.5",
    "@wdio/mocha-framework": "^7.19.5",
    "@wdio/spec-reporter": "^7.19.5",
    "@wdio/static-server-service": "^7.19.5",
    "babel-loader": "^8.2.5",
    "blake2b-no-wasm": "2.1.4",
    "chromedriver": "^102.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "expect-webdriverio": "^3.2.1",
    "jest": "^28.1.1",
    "jest-webextension-mock": "^3.7.22",
    "mock-browser": "^0.92.14",
    "readable-stream": "^3.6.0",
    "shx": "^0.3.3",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4",
    "util": "^0.12.4",
    "wdio-chromedriver-service": "^7.3.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
