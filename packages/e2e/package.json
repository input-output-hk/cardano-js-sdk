    "optionator": "^0.9.1",
    "rxjs": "^7.4.0",
    "ts-log": "^2.2.4",
    "ts-node": "^10.8.1",
    "ts-stopwatch": "0.0.4",
    "uuid": "^8.3.2",
    "webextension-polyfill": "^0.8.0",
    "ws": "^8.5.0"
  },
  "devDependencies": {
