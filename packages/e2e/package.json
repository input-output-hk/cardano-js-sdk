    "expect-webdriverio": "^3.2.1",
    "jest": "^28.1.3",
    "jest-webextension-mock": "^3.7.22",
    "json-bigint": "^1.0.0",
    "mock-browser": "^0.92.14",
    "npm-run-all": "^4.1.5",
    "readable-stream": "^3.6.0",
    "shx": "^0.3.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4",
    "util": "^0.12.4",
