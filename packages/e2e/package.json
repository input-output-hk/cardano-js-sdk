    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@cardano-sdk/dapp-connector": "^0.6.0",
    "@cardano-sdk/util-dev": "^0.5.0",
    "@cardano-sdk/web-extension": "^0.6.0",
    "@dcspark/cardano-multiplatform-lib-asmjs": "^3.1.0",
    "@emurgo/cardano-message-signing-asmjs": "^1.0.1",
    "@types/bunyan": "^1.8.8",
    "@types/jest": "^28.1.2",
    "@types/lodash": "^4.14.182",
    "@types/uuid": "^8.3.4",
    "@types/webextension-polyfill": "^0.8.0",
    "@vespaiach/axios-fetch-adapter": "^0.3.0",
    "@wdio/cli": "^7.19.5",
    "@wdio/local-runner": "^7.19.5",
    "@wdio/mocha-framework": "^7.19.5",
    "@wdio/spec-reporter": "^7.19.5",
    "@wdio/static-server-service": "^7.19.5",
    "babel-loader": "^8.2.5",
    "blake2b-no-wasm": "2.1.4",
    "buffer": "^6.0.3",
    "chromedriver": "^104.0.0",
