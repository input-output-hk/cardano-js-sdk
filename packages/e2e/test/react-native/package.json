{
  "name": "@cardano-sdk/mobile",
  "version": "0.0.1",
  "private": true,
  "repository": "https://github.com/input-output-hk/cardano-js-sdk",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "pod-install": "cd ios && pod install && cd ..",
    "start": "react-native start",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "detox:build:android-debug": "detox build --configuration android.emu.debug",
    "detox:build:ios-debug": "detox build --configuration ios.sim.debug",
    "detox:build:android-release": "detox build --configuration android.emu.release",
    "detox:build:ios-release": "detox build --configuration ios.sim.release",
    "detox:test:android-debug": "detox test --configuration android.emu.debug",
    "detox:test:ios-debug": "detox test --configuration ios.sim.debug",
    "detox:test:android-release": "detox test --configuration android.emu.release",
    "detox:test:ios-release": "detox test --configuration ios.sim.release",
    "clean-artifacts": "rm -rf ./e2e/artifacts",
    "clean-allure": "rm -rf ./e2e/allure-results ./e2e/allure-report",
    "serve-allure-report": "allure serve ./e2e/allure-results",
    "generate-allure-report": "allure generate ./e2e/allure-results --clean -o ./e2e/allure-report"
  },
  "dependencies": {
    "detox": "20.1.2",
    "react": "18.2.0",
    "react-native": "0.71.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli-platform-android": "^10.1.3",
    "@react-native-community/cli-platform-ios": "^10.1.1",
    "@react-native-community/eslint-config": "^3.0.0",
    "@rnx-kit/metro-config": "^1.3.5",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.26",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.2",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "allure-commandline": "^2.21.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-plugin-detox": "^1.0.0",
    "jest": "^29.2.1",
    "jest-allure2-reporter": "^1.2.1",
    "jsc-android": "^250230.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-native-gradle-plugin": "^0.71.14",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
